<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>tc9_eau__STP_Email_ATO_processing_error</fullName>
        <description>STP Email - ATO processing error</description>
        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>tc9_eau__Lodgement_Emails/tc9_eau__STP_Error_from_ATO</template>
    </alerts>
    <rules>
        <fullName>tc9_eau__STP Email - ATO Error</fullName>
        <actions>
            <name>tc9_eau__STP_Email_ATO_processing_error</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Send an email when a STP Lodgement record cannot be processed by the ATO.</description>
        <formula>tc9_eau__ATO_Error__c = True</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
